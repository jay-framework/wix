  "dependencies": {
    "@jay-framework/component": "^0.6.10",
    "@jay-framework/fullstack-component": "^0.6.10",
    "@jay-framework/reactive": "^0.6.10",
    "@jay-framework/runtime": "^0.6.10",
    "@jay-framework/secure": "^0.6.10",
    "@jay-framework/wix-server-client": "^1.0.0",
    "@wix/stores": "^1.0.547"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@babel/preset-env": "^7.23.8",
    "@babel/preset-typescript": "^7.23.3",
    "@jay-framework/jay-cli": "^0.6.10",
    "@jay-framework/vite-plugin": "^0.6.10",
    "nodemon": "^3.0.3",
    "rimraf": "^5.0.5",
    "tslib": "^2.6.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-inspect": "^0.8.1"
  }
