{
  "name": "@jay-framework/wix-stores",
  "version": "1.0.0",
  "type": "module",
  "description": "Wix Stores API client for Jay Framework",
  "private": true,
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "build": "npm run definitions && npm run build:js && npm run build:copy-contract",
    "definitions": "jay-cli definitions lib",
    "build:js": "vite build",
